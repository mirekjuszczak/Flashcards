<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>

<ResourceDictionary
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!--#region Light Theme Colors-->
    <Color x:Key="TextPrimaryLight">#ff2d2d2d</Color>
    <Color x:Key="TextSecondaryLight">#ff5a5a5a</Color>
    <Color x:Key="TextTertiaryLight">#ff7a7a7a</Color>
    <Color x:Key="BackgroundPrimaryLight">#fffbf3d5</Color>
    <Color x:Key="SurfaceLight">#ffd6dac8</Color>
    <Color x:Key="SurfaceSecondaryLight">#ffd6dac8</Color>
    <Color x:Key="AccentPrimaryLight">#ffd6a99d</Color>
    <!--#endregion-->

    <!--#region Dark Theme Colors-->
    <Color x:Key="TextPrimaryDark">#fffbf3d5</Color>
    <Color x:Key="TextSecondaryDark">#ffd6dac8</Color>
    <Color x:Key="TextTertiaryDark">#ff9cafaa</Color>
    <Color x:Key="BackgroundPrimaryDark">#ff3d4a47</Color>
    <Color x:Key="SurfaceDark">#ff5a6b65</Color>
    <Color x:Key="SurfaceSecondaryDark">#ff5a6b65</Color>
    <Color x:Key="AccentPrimaryDark">#ffd6a99d</Color>
    <!--#endregion-->

</ResourceDictionary>
